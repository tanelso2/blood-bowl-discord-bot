{
  "name": "discord_bot_template",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile",
    "clean": "rm -r build && rm -r node_modules",
    "check-all": "npm run build && npm run test && npm run lint",
    "compile": "tsc --project ./",
    "test": "LOG_LEVEL=debug ts-mocha --paths --file ./src/tests/init \"**/*.spec*.ts\" --ignore \"node_modules/**/*\"",
    "dev": "npm run build && LOG_LEVEL=debug node ./build/ | npx pino-pretty",
    "lint": "eslint --fix-dry-run ."
  },
  "author": "",
  "license": "Unlicense",
  "dependencies": {
    "@types/node": "^18.11.10",
    "discord.js": "^14.7.0",
    "js-yaml": "^3.14.0",
    "module-alias": "^2.2.2",
    "pino": "^8.7.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "chai": "^4.3.0",
    "eslint": "^7.25.0",
    "jsdoc": "^3.6.10",
    "mocha": "^10.0.0",
    "pino-pretty": "^4.8.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.2.2",
    "typescript-formatter": "^7.2.2"
  },
  "_moduleAliases": {
    "@commands": "build/src/commands",
    "@core": "build/src/core",
    "@formatting": "build/src/formatting",
    "@generator": "build/src/generator",
    "@models": "build/src/models",
    "@odds": "build/src/odds",
    "@utils": "build/src/utils"
  }
}
